{
  "extends": "../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "app",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "app",
      "kebab-case"
    ],
    "angular-whitespace": [
      true,
      "check-interpolation",
      "check-semicolon"
    ]
  }
}
